{

    "manifest_version": 2,
    "name": "Nude-Moon blur remove",
    "version": "0.1",
    "description": "Nude-Moon cover blur remove",
    "applications": {
        "gecko": {
            "id": "nudemoonblurremove@nyako.icu"
        }
    },
    "content_scripts": [
        {
        "matches": ["*://*.nude-moon.net/*", "*://nude-moon.net/*"],
        "js": ["main-js.js"]
        }
    ],
    "permissions": ["nativeMessaging"]
}